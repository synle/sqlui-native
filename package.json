{
  "name": "sqlui-native",
  "version": "1.64.2",
  "private": true,
  "description": "A minimal native desktop client for most databases supporting MySQL, MariaDB, MS SQL Server, PostgresSQL, SQLite, Cassandra, MongoDB and Redis.",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3001",
  "homepage": "./",
  "build": {
    "extends": null,
    "appId": "io.synle.github.sqlui-native",
    "files": [
      "**/*.icns",
      "**/*.ico",
      "**/*.jpg",
      "**/*.png",
      "*.css",
      "*.js",
      "*.json",
      "static/**/*",
      "appx/**/*",
      "assets/**/*",
      "index.html",
      "vs/**/*",
      "!src/**/*",
      "!public/**/*",
      "!typings/**/*",
      "!scripts/**/*",
      "!mocked-server.js",
      "!robots.txt",
      "!mocked-server.js.LICENSE.txt"
    ],
    "win": {
      "icon": "build/logo512.png"
    },
    "appx": {
      "publisherDisplayName": "Sy Le",
      "applicationId": "sqluiNative"
    },
    "mac": {
      "icon": "build/logo512.png"
    },
    "afterPack": "./scripts/afterPack.js",
    "linux": {
      "synopsis": "sqlui-native - A minimal native desktop client for most databases supporting MySQL, MariaDB, MS SQL Server, PostgresSQL, SQLite, Cassandra, MongoDB and Redis.",
      "icon": "build/logo256.icns",
      "category": "Development",
      "target": [
        "deb",
        "rpm",
        "snap",
        "pacman"
      ]
    }
  },
  "main": "main.js",
  "scripts": {
    "### Cleanup & Maintenance ###": "##################################################################################",
    "clean": "rm -rf build public/vs src/package.json",
    "cleanSnapshot": "rm -rf src/common/adapters/__snapshots__/ src/common/adapters/BaseDataAdapter/__snapshots__/ && npm run test-ci -- --update",
    "format": "prettier --write --cache --ignore-unknown --no-error-on-unmatched-pattern --print-width 140 .",
    "lint": "eslint --fix --ext .tsx --ext .ts src",
    "typecheck": "tsc --noEmit",
    "### Build Lifecycle ###": "##################################################################################",
    "prebuild": "node scripts/prebuild.js",
    "build": "cross-env CI=\"\" react-scripts build",
    "postbuild": "concurrently \"npm run build-electron\" \"npm run build-mocked-server\"",
    "build-electron": "webpack --config webpack-electron.config.js",
    "build-mocked-server": "webpack --config webpack-mocked-server.config.js",
    "### Development & Execution ###": "##################################################################################",
    "predev": "node scripts/prebuild.js",
    "dev": "concurrently \"npm run start-mocked-server\" \"npm run start-webapp\"",
    "prestart": "npm run build",
    "start": "npm run start-electron",
    "start-electron": "cross-env ENV_TYPE='electron-dev' electron build/main.js",
    "start-webapp": "cross-env ENV_TYPE='mocked-server' BROWSER='none' react-scripts start",
    "prestart-mocked-server": "npm run build-mocked-server",
    "start-mocked-server": "node build/mocked-server.js",
    "### Testing ###": "##################################################################################",
    "pretest": "npm run build",
    "test": "vitest --watch",
    "pretest-ci": "npm run pretest",
    "test-ci": "vitest run",
    "### Distribution & Packaging ###": "##################################################################################",
    "dist": "electron-builder build --publish=never",
    "dist-win-x64": "electron-builder build --publish=never --win --x64",
    "dist-win-arm64": "electron-builder build --publish=never --win --arm64 --config.npmRebuild=false",
    "dist-macos-x64": "electron-builder build --publish=never --mac --x64",
    "dist-macos-arm64": "electron-builder build --publish=never --mac --arm64",
    "dist-linux-appimage": "electron-builder build --publish=never -l AppImage",
    "dist-win32-appx": "electron-builder build --publish=never -w appx"
  },
  "repository": "https://github.com/synle/sqlui-native",
  "keywords": [
    "Electron",
    "SQL",
    "Native Desktop Application",
    "MySQL",
    "MariaDB",
    "Microsoft",
    "PostgresSQL",
    "SQLite",
    "Cassandra"
  ],
  "license": "MIT",
  "author": {
    "name": "Sy Le",
    "email": "le.nguyen.sy@gmail.com"
  },
  "dependencies": {
    "@azure/cosmos": "^3.17.1",
    "@azure/data-tables": "^13.1.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.8.2",
    "@mui/lab": "^5.0.0-alpha.100",
    "@mui/material": "^5.10.8",
    "@tanstack/react-query": "^4.43.0",
    "@tanstack/react-query-devtools": "^4.43.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.18",
    "cassandra-driver": "^3.6.0",
    "connection-string-parser": "^1.0.4",
    "eslint-plugin-unused-imports": "^4.4.1",
    "fuzzysort": "^1.2.1",
    "html-to-image": "^1.10.2",
    "js-beautify": "^1.14.3",
    "json-2-csv": "^3.17.1",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "mariadb": "^3.1.2",
    "monaco-editor": "^0.32.1",
    "mongodb": "^4.10.0",
    "mysql2": "^2.3.3",
    "notistack": "^2.0.5",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "qs": "^6.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-flow-renderer": "^10.3.14",
    "react-router-dom": "^6.2.2",
    "redis": "^4.3.1",
    "sequelize": "^6.20.1",
    "sql-formatter": "^4.0.2",
    "sqlite3": "^5.1.6",
    "tedious": "^16.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@testing-library/react": "^12.1.5",
    "@types/cassandra-driver": "^3.6.0",
    "@types/js-beautify": "^1.13.3",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^2.0.12",
    "body-parser": "^2.2.2",
    "concurrently": "^7.4.0",
    "cross-env": "^7.0.3",
    "electron": "^28.3.3",
    "electron-builder": "^25.1.8",
    "eslint": "^8.35.0",
    "express": "^4.18.1",
    "multer": "^1.4.4",
    "prettier": "^3.8.1",
    "react-scripts": "5.0.1",
    "sass": "^1.62.1",
    "supertest": "^6.3.3",
    "ts-loader": "^9.3.0",
    "typescript": "^4.9.5",
    "vitest": "^0.34.6",
    "web-vitals": "^3.3.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  }
}
