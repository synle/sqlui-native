{
  "name": "sqlui-native",
  "version": "1.0.0",
  "description": "A minimal Electron application for SQL Databases",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "./",
  "scripts": {
    "clean": "rimraf .cache .parcel-cache dist build",
    "start": "cross-env ELECTRON_DISABLE_SECURITY_WARNINGS=true electron ./main.js",
    "start-webapp": "cross-env SASS_PATH='./src/styles' react-scripts start",
    "start-mocked": "npx ts-node --project tsconfig-server.json mocked-server.ts",
    "build": "cross-env SASS_PATH='./src/styles' react-scripts build",
    "test": "cross-env SASS_PATH='./src/styles' react-scripts test",
    "format": "npx prettier --config ./.prettierrc --write src/**/**/**/**/*.{ts,tsx,js,jsx,scss,yml} *.{json,MD}",
    "eject": "react-scripts eject"
  },
  "repository": "https://github.com/synle/sqlui-native",
  "keywords": [
    "Electron",
    "SQL"
  ],
  "author": "Sy Le",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.12",
    "react-router-dom": "^6.2.1",
    "sequelize": "^6.14.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "cross-env": "^7.0.3",
    "electron": "^16.0.7",
    "express": "^4.17.2",
    "mariadb": "^2.5.5",
    "mysql2": "^2.3.3",
    "pg": "^8.7.1",
    "react-scripts": "5.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.49.0",
    "sqlite3": "^5.0.2",
    "tedious": "^14.1.0",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4"
  }
}
