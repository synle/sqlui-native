{
  "name": "sqlui-native",
  "version": "1.19.0",
  "description": "A minimal native desktop client for most databases supporting MySQL, MariaDB, MS SQL Server, PostgresSQL, SQLite, Cassandra, MongoDB and Redis.",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3001",
  "homepage": "./",
  "build": {
    "appId": "io.synle.github.sqlui-native",
    "extends": null,
    "win": {
      "icon": "build/logo512.png"
    },
    "mac": {
      "icon": "build/logo512.png"
    },
    "linux": {
      "synopsis": "sqlui-native is a minimal native desktop client for most databases supporting MySQL, MariaDB, MS SQL Server, PostgresSQL, SQLite, Cassandra, MongoDB and Redis.",
      "icon": "build/logo512.png",
      "category": "Development",
      "target": [
        "deb",
        "rpm",
        "snap"
      ]
    }
  },
  "main": "main.js",
  "scripts": {
    "clean": "rimraf .cache .parcel-cache dist build mocked-db.sqlite public/vs && cpx package.json src && cpx \"node_modules/monaco-editor/min/vs/**/*.{css,js}\"  ./public/vs",
    "cleanSnapshot": "rm -rf __tests__/__snapshots__/ && npm run test-ci",
    "prestart": "npm run clean && npm run build",
    "prebuild": "npm run clean",
    "predev": "npm run prebuild",
    "pretest": "npm run prebuild",
    "pretest-ci": "npm run pretest",
    "predist": "cpx \"build/**/*\" .",
    "preformat": "curl https://raw.githubusercontent.com/synle/js-import-fixer/main/removed-unused-imports.js | node - --groupImport --aggressive",
    "start": "npm run start-electron",
    "start-electron": "cross-env ENV_TYPE='electron-dev' electron build/main.js",
    "start-webapp": "cross-env ENV_TYPE='mocked-server' react-scripts start",
    "start-mocked": "npx ts-node --project tsconfig-server.json mocked-server.ts",
    "dev": "concurrently \"npm run start-mocked\" \"npm run start-webapp\"",
    "dist": "electron-builder build --publish=never",
    "build-electron": "tsc --project tsconfig-electron.json && cpx package.json build",
    "build-react": "cross-env CI=\"\" react-scripts build",
    "build": "npm run build-react && npm run build-electron",
    "test": "cross-env CI=true npm test-electron && cross-env CI=true npm run test-react",
    "test-ci": "cross-env CI=true jest __tests__ && cross-env CI=true npm run test-react",
    "test-electron": "jest commons __tests__",
    "test-react": "cross-env react-scripts test",
    "format": "npx prettier --config ./.prettierrc --write **/*.{ts,tsx,js,jsx,scss,yml,html,json,md}",
    "lint": "eslint --fix --ext .tsx --ext .ts src"
  },
  "repository": "https://github.com/synle/sqlui-native",
  "keywords": [
    "Electron",
    "SQL",
    "Native Desktop Application",
    "MySQL",
    "MariaDB",
    "Microsoft",
    "PostgresSQL",
    "SQLite",
    "Cassandra"
  ],
  "license": "MIT",
  "author": {
    "name": "Sy Le",
    "email": "le.nguyen.sy@gmail.com"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.4.1",
    "@mui/material": "^5.4.0",
    "cassandra-driver": "^3.6.0",
    "connection-string-parser": "^1.0.4",
    "fuzzysort": "^1.1.4",
    "js-beautify": "^1.14.0",
    "json-2-csv": "^3.17.0",
    "mariadb": "^2.5.5",
    "monaco-editor": "^0.32.1",
    "mongodb": "^4.3.1",
    "mysql2": "^2.3.3",
    "pg": "^8.7.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.15",
    "react-router-dom": "^6.2.1",
    "react-table": "^7.7.0",
    "redis": "^4.0.3",
    "sequelize": "^6.16.1",
    "sql-formatter": "^4.0.2",
    "sqlite3": "^5.0.2",
    "tedious": "^14.3.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/cassandra-driver": "^3.6.0",
    "@types/jest": "^27.4.0",
    "@types/js-beautify": "^1.13.3",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-table": "^7.7.9",
    "@types/redis": "^4.0.11",
    "body-parser": "^1.19.1",
    "concurrently": "^7.0.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "electron": "^17.0.0",
    "electron-builder": "^22.14.13",
    "eslint-plugin-unused-imports": "^2.0.0",
    "express": "^4.17.2",
    "jest": "^27.5.1",
    "multer": "^1.4.4",
    "react-scripts": "5.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.49.7",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4"
  }
}
