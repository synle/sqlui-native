{
  "name": "sqlui-native",
  "version": "1.39.0",
  "description": "A minimal native desktop client for most databases supporting MySQL, MariaDB, MS SQL Server, PostgresSQL, SQLite, Cassandra, MongoDB and Redis.",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3001",
  "homepage": "./",
  "build": {
    "appId": "io.synle.github.sqlui-native",
    "extends": null,
    "win": {
      "icon": "build/logo512.png"
    },
    "mac": {
      "icon": "build/logo512.png"
    },
    "linux": {
      "synopsis": "sqlui-native is a minimal native desktop client for most databases supporting MySQL, MariaDB, MS SQL Server, PostgresSQL, SQLite, Cassandra, MongoDB and Redis.",
      "icon": "build/logo512.png",
      "category": "Development",
      "target": [
        "deb",
        "rpm",
        "snap"
      ]
    }
  },
  "main": "main.js",
  "scripts": {
    "clean": "rimraf upload .cache .parcel-cache dist build mocked-db.sqlite public/vs && cpx package.json src && cpx \"node_modules/monaco-editor/min/vs/**/*.{css,js}\" ./public/vs",
    "cleanSnapshot": "rm -rf __tests__/__snapshots__/ && npm run test-ci",
    "prestart": "npm run clean && npm run build",
    "prebuild": "npm run clean",
    "predev": "npm run prebuild",
    "prestart-mocked-server": "npm run build-mocked-sever",
    "pretest": "npm run prebuild",
    "pretest-ci": "npm run pretest",
    "predist": "cpx \"build/**/*\" .",
    "start": "npm run start-electron",
    "start-electron": "cross-env ENV_TYPE='electron-dev' electron build/main.js",
    "start-webapp": "cross-env ENV_TYPE='mocked-server' BROWSER='none' react-scripts start",
    "start-mocked-server": "node build/mocked-server.js",
    "dev": "concurrently \"npm run start-mocked-server\" \"npm run start-webapp\"",
    "dist": "electron-builder build --publish=never",
    "build-electron": "webpack --config webpack-electron.config.js && cpx package.json build",
    "build-mocked-sever": "webpack --config webpack-mocked-server.config.js",
    "build-react": "cross-env CI=\"\" react-scripts build",
    "build": "npm run build-react && concurrently \"npm run build-electron\" \"npm run build-mocked-sever\"",
    "test-ci": "cross-env CI=true jest",
    "test": "jest --watch --verbose",
    "fix-import": "npx import-fixer --groupImport --aggressive --transformRelativeImport --importQuote=single",
    "preformat": "npm run fix-import",
    "format": "npx prettier --config ./.prettierrc --no-error-on-unmatched-pattern --write **/*.{ts,tsx,js,jsx,scss,yml,html,json,md}",
    "lint": "eslint --fix --ext .tsx --ext .ts src"
  },
  "repository": "https://github.com/synle/sqlui-native",
  "keywords": [
    "Electron",
    "SQL",
    "Native Desktop Application",
    "MySQL",
    "MariaDB",
    "Microsoft",
    "PostgresSQL",
    "SQLite",
    "Cassandra"
  ],
  "license": "MIT",
  "author": {
    "name": "Sy Le",
    "email": "le.nguyen.sy@gmail.com"
  },
  "dependencies": {
    "@azure/cosmos": "^3.16.1",
    "@azure/data-tables": "^13.1.1",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.2",
    "@mui/material": "^5.8.2",
    "cassandra-driver": "^3.6.0",
    "connection-string-parser": "^1.0.4",
    "fuzzysort": "^1.2.1",
    "js-beautify": "^1.14.3",
    "json-2-csv": "^3.17.1",
    "mariadb": "^3.0.0",
    "monaco-editor": "^0.32.1",
    "mongodb": "^4.6.0",
    "mysql2": "^2.3.3",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.39.1",
    "react-router-dom": "^6.2.2",
    "react-simple-resizer": "git+https://github.com/synle/react-simple-resizer.git",
    "react-table": "^7.8.0",
    "redis": "^4.1.0",
    "sequelize": "^6.20.1",
    "sql-formatter": "^4.0.2",
    "sqlite3": "^5.0.8",
    "tedious": "^14.5.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.1.1",
    "@types/cassandra-driver": "^3.6.0",
    "@types/jest": "^27.4.1",
    "@types/js-beautify": "^1.13.3",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@types/react-table": "^7.7.11",
    "@types/redis": "^4.0.11",
    "body-parser": "^1.20.0",
    "concurrently": "^7.2.1",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "electron": "^19.0.2",
    "electron-builder": "^23.0.3",
    "eslint-plugin-unused-imports": "^2.0.0",
    "express": "^4.18.1",
    "jest": "^27.5.1",
    "multer": "^1.4.4",
    "react-scripts": "5.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.51.0",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "web-vitals": "^2.1.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  }
}
