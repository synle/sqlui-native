{
  "name": "sqlui-native",
  "version": "1.0.0",
  "description": "A minimal Electron application for SQL Databases",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "main": "main.js",
  "scripts": {
    "prebuild": "rm -rf ./dist ./build",
    "start": "electron .",
    "start-webapp": "SASS_PATH='./src/styles' parcel src/index.html --port 3000",
    "build": "SASS_PATH='./src/styles' parcel build src/index.html --out-dir dist --public-url ./",
    "format": "npx prettier --config ./.prettierrc --write src/**/**/**/**/*.{ts,tsx,js,jsx,scss,yml} *.{json,MD}",
    "lint": "eslint --ext js,jsx,ts,tsx src",
    "test": "echo \"Error: no test specified\""
  },
  "repository": "https://github.com/synle/sqlui-native",
  "keywords": [
    "Electron",
    "SQL"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "electron": "^16.0.7",
    "eslint": "^8.7.0",
    "eslint-plugin-react": "^7.28.0",
    "parcel-bundler": "^1.12.5",
    "sass": "^1.49.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1"
  }
}
